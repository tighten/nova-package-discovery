{
    "name": "tightenco/nova-package-discovery",
    "description": "Discover the newest and most popular Nova packages.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "kitetail/zttp": "^0.3"
    },
    "autoload": {
        "psr-4": {
            "Tightenco\\NovaPackageDiscovery\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tightenco\\NovaPackageDiscovery\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
